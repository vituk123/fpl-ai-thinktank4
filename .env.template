# FPL Optimizer Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Database Connection String (Pooler)
DB_CONNECTION_STRING=postgresql://postgres.your-project-id:your_password@aws-your-region.pooler.supabase.com:5432/postgres

# Optional: Override default logging level
LOG_LEVEL=INFO