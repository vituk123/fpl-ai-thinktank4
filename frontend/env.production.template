# Production Environment Variables Template
# Copy this file to .env.production and fill in your actual values

# Render FastAPI Backend URL (after deployment)
VITE_API_BASE_URL=https://your-render-app.onrender.com/api/v1

# Supabase Edge Functions URL
VITE_SUPABASE_FUNCTIONS_URL=https://your-project.supabase.co/functions/v1

# Supabase Project URL
VITE_SUPABASE_URL=https://your-project.supabase.co

# Supabase Anon Key (from Supabase Dashboard > Settings > API)
VITE_SUPABASE_ANON_KEY=your-production-anon-key

